{
  "version": 2,
  "installCommand": "echo skip install",
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/paths", "dest": "/api/paths.js" },
    { "src": "/api/list-files", "dest": "/api/list-files.js" },
    { "src": "/api/validate", "dest": "/api/validate.js" },
    { "src": "/api/submit", "dest": "/api/submit.js" },
    { "src": "/api/status", "dest": "/api/status.js" },
    { "src": "/api/github-device-start", "dest": "/api/github-device-start.js" },
    { "src": "/api/github-device-token", "dest": "/api/github-device-token.js" },
    { "src": "/api/admin/auth", "dest": "/api/admin/auth.js" },
    { "src": "/api/admin/submissions", "dest": "/api/admin/submissions.js" },
    { "src": "/api/admin/submission-details", "dest": "/api/admin/submission-details.js" },
    { "src": "/api/admin/approve", "dest": "/api/admin/approve.js" },
    { "src": "/api/admin/reject", "dest": "/api/admin/reject.js" }
  ]
}
